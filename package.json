{
  "name": "aime-blueprint",
  "title": "Blueprint",
  "description": "Standard components and UI design definitions for all AIME projects",
  "version": "2.0.17",
  "license": "MIT",
  "homepage": "https://blueprint.aimementoring.com",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "engines": {
    "node": ">=10.0"
  },
  "dependencies": {
    "@bugsnag/js": "^6.2.0",
    "@bugsnag/plugin-react": "^6.2.0",
    "@svgr/webpack": "^4.2.0",
    "airtable": "^0.6.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "bfj": "^6.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "cross-fetch": "^3.0.2",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.4.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-intl-tel-input": "^7.0.1",
    "react-player": "^1.11.0",
    "react-select": "^2.4.3",
    "resolve": "^1.10.1",
    "styled-jsx": "^3.2.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@svgr/rollup": "^4.2.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-react-app": "^9.0.0",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-flowtype": "^3.8.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.8.0",
    "lint-staged": "^8.1.6",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "react-styleguidist": "^9.0.9",
    "react-test-renderer": "^16.8.6",
    "release-it": "^12.0.1",
    "rollup": "^1.11.3",
    "rollup-plugin-analyzer": "^3.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-delete": "^1.0.0",
    "rollup-plugin-execute": "^1.0.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-url": "^2.2.1",
    "rollup-plugin-visualizer": "^1.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "yarn-upgrade-all": "^0.5.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint --ext .js",
    "**/*.less": "stylelint --syntax scss"
  },
  "scripts": {
    "start": "PORT=3001 node scripts/start.js",
    "start:styleguide": "styleguidist server",
    "build:styleguide": "styleguidist build",
    "build:lib": "cross-env NODE_ENV=production BABEL_ENV=production rollup -c",
    "build": "yarn build:lib && yarn build:styleguide",
    "lint-staged": "lint-staged",
    "lint": "eslint --ext .js src",
    "fix-lint": "eslint --fix --ext .js",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix-styles": "prettier-stylelint --write 'src/**/*.{css,scss}' ",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest --coverage",
    "test:update-snapshot": "cross-env NODE_ENV=test BABEL_ENV=test jest -u",
    "deploy": "yarn test && yarn build",
    "release": "release-it",
    "upgrade-all": "yarn yarn-upgrade-all",
    "coveralls": "node node_modules/.bin/coveralls"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "files": [
    "lib",
    "README.md",
    "styleguide"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aimementoring/blueprint.git"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "author": {
    "name": "Kevin Bardi",
    "email": "bardi.kevin@gmail.com"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "styled-jsx/babel"
      ]
    ]
  },
  "release-it": {
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
